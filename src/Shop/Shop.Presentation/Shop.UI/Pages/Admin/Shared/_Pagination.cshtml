@model Common.Query.BaseClasses.FilterQuery.BaseFilter

<ul class="pagination">
  @if (Model.CurrentPage > 1)
  {
    <li class="page-item previous">
      <a href="javascript:void(0)" class="page-link"
         onclick="setUrlParam('FilterParams.PageId', @(Model.CurrentPage - 1))">
        قبلی
      </a>
    </li>
  }
  @for (var i = Model.StartPage; i <= Model.EndPage; i++)
  {
    <li class="page-item @(i == Model.CurrentPage ? "active" : null)">
      <a href="javascript:void(0)" class="page-link" onclick="setUrlParam('FilterParams.PageId', @i)">
        @i
      </a>
    </li>
  }
  @if (Model.PageCount != 0 && Model.CurrentPage != Model.PageCount)
  {
    <li class="page-item next">
      <a href="javascript:void(0)" class="page-link"
         onclick="setUrlParam('FilterParams.PageId', @(Model.CurrentPage + 1))">
        بعدی
      </a>
    </li>
  }
</ul>