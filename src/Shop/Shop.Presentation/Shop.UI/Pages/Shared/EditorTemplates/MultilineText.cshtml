@model string

<div class="form-element-row">
    <div class="text-sm text-muted mb-1">@Html.DisplayNameFor(m => m):</div>
    <div class="text-dark font-weight-bold">
        <div class="form-element-row mb-0">
            <textarea asp-for="@Model" cols="30" rows="10" class="input-element multiline-text"></textarea>
        </div>
    </div>
</div>
<span asp-validation-for="@Model" class="d-block mb-2"></span>